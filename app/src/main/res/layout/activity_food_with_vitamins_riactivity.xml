<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".FoodWithVitaminsRIActivity">

    <ImageView
        android:id="@+id/backBtnRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backarrow" />

    <TextView
        android:id="@+id/appNameRI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_name"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.056" />

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="415dp"
        android:layout_height="73dp"
        android:layout_marginBottom="-10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/homebarbackground" />

    <ImageView
        android:id="@+id/scanBtnRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/scanbutton" />

    <ImageView
        android:id="@+id/searchBtnRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/searchicon" />

    <ImageView
        android:id="@+id/profileBtnRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginBottom="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/usericon" />

    <ImageView
        android:id="@+id/toggleRiRi"
        android:layout_width="186dp"
        android:layout_height="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView37"
        app:layout_constraintEnd_toEndOf="@+id/imageView37"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView37"
        app:layout_constraintTop_toTopOf="@+id/imageView37"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/whitebox" />

    <ImageView
        android:id="@+id/imageView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toTopOf="@+id/firstFoodPicRi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/toggleouter" />

    <ImageView
        android:id="@+id/toggleGramRi"
        android:layout_width="186dp"
        android:layout_height="28dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView37"
        app:layout_constraintEnd_toEndOf="@+id/imageView37"
        app:layout_constraintTop_toTopOf="@+id/imageView37"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/bluebox" />

    <ImageView
        android:id="@+id/firstFoodPicRi"
        android:layout_width="118dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/secondFoodPicRi"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/foodsribellpepper" />

    <ImageView
        android:id="@+id/secondFoodPicRi"
        android:layout_width="139dp"
        android:layout_height="96dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="-7dp"
        app:layout_constraintBottom_toTopOf="@+id/thirdFoodPicRi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.975"
        app:srcCompat="@drawable/foodsrimashedpotatoes" />

    <ImageView
        android:id="@+id/thirdFoodPicRi"
        android:layout_width="141dp"
        android:layout_height="101dp"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/fourthFoodPicRi"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/foodsripopcorn" />

    <TextView
        android:id="@+id/firstFoodTextRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text=""
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/firstFoodPicRi"
        app:layout_constraintStart_toEndOf="@+id/firstFoodPicRi"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.993" />

    <TextView
        android:id="@+id/firstFoodNameRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="8dp"
        android:text=""
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/firstFoodTextRi"
        app:layout_constraintStart_toEndOf="@+id/firstFoodPicRi" />

    <TextView
        android:id="@+id/secondFoodNameRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="8dp"
        android:text=""
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/secondFoodTextRi"
        app:layout_constraintStart_toEndOf="@+id/secondFoodPicRi" />

    <TextView
        android:id="@+id/secondFoodTextRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="8dp"
        android:text=""
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/secondFoodPicRi"
        app:layout_constraintStart_toEndOf="@+id/secondFoodPicRi" />

    <TextView
        android:id="@+id/thirdFoodTextRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text=""
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/thirdFoodPicRi"
        app:layout_constraintStart_toEndOf="@+id/thirdFoodPicRi" />

    <TextView
        android:id="@+id/thirdFoodNameRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="8dp"
        android:text=""
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/thirdFoodTextRi"
        app:layout_constraintStart_toEndOf="@+id/thirdFoodPicRi" />

    <TextView
        android:id="@+id/fourthFoodNameRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="12dp"
        android:text=""
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/fourthFoodTextRi"
        app:layout_constraintStart_toEndOf="@+id/fourthFoodPicRi" />

    <TextView
        android:id="@+id/fourthFoodTextRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:text=""
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/fourthFoodPicRi"
        app:layout_constraintStart_toEndOf="@+id/fourthFoodPicRi" />

    <TextView
        android:id="@+id/riTextRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/FoodWithToggle"
        android:textColor="#2147B1"
        app:layout_constraintBottom_toBottomOf="@+id/toggleRiRi"
        app:layout_constraintEnd_toEndOf="@+id/toggleRiRi"
        app:layout_constraintStart_toStartOf="@+id/toggleRiRi"
        app:layout_constraintTop_toTopOf="@+id/toggleRiRi" />

    <TextView
        android:id="@+id/gramTextRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/RI_Toggle"
        android:textColor="#FEFEFE"
        app:layout_constraintBottom_toBottomOf="@+id/toggleGramRi"
        app:layout_constraintEnd_toEndOf="@+id/imageView37"
        app:layout_constraintStart_toStartOf="@+id/toggleGramRi"
        app:layout_constraintTop_toTopOf="@+id/toggleGramRi" />

    <ImageView
        android:id="@+id/fourthFoodPicRi"
        android:layout_width="115dp"
        android:layout_height="106dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="56dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView32"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/foodsrikoriander" />

    <TextView
        android:id="@+id/whatIsVitaminRi"
        android:layout_width="343dp"
        android:layout_height="144dp"
        android:layout_marginBottom="20dp"
        android:text="@string/whatIsCVitamin"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/imageView37"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appNameRI"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/vitaminNameRi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="TextView"
        android:textColor="#000000"
        app:layout_constraintBottom_toTopOf="@+id/whatIsVitaminRi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appNameRI"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>